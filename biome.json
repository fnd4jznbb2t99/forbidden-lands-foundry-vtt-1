{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
		"enabled": false
	},
	"formatter": {
		"ignore": ["./forbidden-lands.js", "./types/foundry/*"]
	},
	"linter": {
		"enabled": true,
		"ignore": ["./forbidden-lands.js", "./types/foundry/*"],
		"rules": {
			"recommended": true,
			"style": {
				"noParameterAssign": "off"
			},
			"complexity": {
				"noStaticOnlyClass": "off",
				"noThisInStatic": "off",
				"useLiteralKeys": "off"
			}
		}
	}
}
